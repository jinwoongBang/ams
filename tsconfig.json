{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",                     
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./build",
    "strict": true,                          
    "noImplicitAny": true,         
    "baseUrl": "./src",             
    "esModuleInterop": true,         
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true  
  }
}
